{
  "version": 2,
  "builds": [
    {
      "src": "frontend/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
        "src": "/",
        "dest": "frontend/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://rubberducker.vercel.app/"
    }
  ]

}
